{
  "name": "rss-puzzle",
  "version": "0.0.1",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
    "build": "webpack --config ./webpack.config.js --env mode=prod",
    "format": "prettier --write src/**/*.{js,ts,tsx,scss,css,json}",
    "ci:format": "prettier --check src/**/*.{js,ts,tsx,scss,css,json}",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "lint:ts": "tsc --noEmit && eslint src/**/*.ts --color",
    "lint-staged": "lint-staged",
    "prepare": "cd .. && npx husky rss-puzzle/.husky"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": ["english","word","puzzle"],
  "author": "ocelopp",
  "license": "MIT",
  "homepage": "",
  "lint-staged": {
    "src/**/*.{js,ts,tsx,scss,css,json}": "prettier --write",
    "src/**/*.{js,ts,tsx}": "eslint --fix --max-warnings=0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-webpack-plugin": "^4.0.1",
    "hasky": "^3.0.2",
    "html-webpack-plugin": "^5.6.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.0",
    "webpack-merge": "^5.10.0"
  }
}
